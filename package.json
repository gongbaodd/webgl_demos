  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@typescript-eslint/eslint-plugin": "^2.22.1-alpha.6",
    "@typescript-eslint/parser": "^2.22.0",
    "audit-ci": "^2.5.1",
    "commitizen": "^4.0.3",
    "emoji-cz": "^0.3.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "serve": "^11.3.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.0",
    "@types/three": "^0.103.2",
    "components": "^0.1.0",
    "lodash": "^4.17.15",
    "three": "^0.114.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type && yarn audit:ci && lint-staged"
    }
  },
  "lint-staged": {
    "*.js(x)|*.json|*.ts(x)|*.markdown|*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "emoji-cz"
    }
  },
  "scripts": {
    "type": "tsc --noEmit",
    "audit:ci": "audit-ci -m -w acorn",
    "commit": "git-cz",
    "test": "echo 'test'",
    "dev": "parcel index.html",
    "build": "parcel build index.html",
    "deploy": "yarn build --no-source-maps && gh-pages -d dist -r https://$GH_TOKEN@git@github.com:gongbaodd/webgl_demos.git -b master"
  }
}
